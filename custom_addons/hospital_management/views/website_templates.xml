<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <template id="hospital_home_template" name="Hospital Home">
        <t t-call="website.layout">
            <!-- Hero Section -->
            <section class="hero-section bg-primary text-white py-5 vh-100 d-flex align-items-center" style="background: linear-gradient(rgba(0,0,0,0.5), rgba(0,0,0,0.5)), url('https://via.placeholder.com/1920x600/007bff/ffffff?text=Hospital+Building') center/cover no-repeat;">
                <div class="container-fluid">
                <h1 class="display-3 fw-bold mb-3">Welcome to Taye Hospital System</h1>
                    <div class="row align-items-center h-100">
                    
                        <div class="col-lg-6">
                            
                            <p class="fs-5 mb-4">Your health is our priority. Comprehensive healthcare services with modern technology and compassionate care.</p>
                            <div class="d-flex gap-3 flex-wrap">                                
                                <t t-if="user_id._is_public()">
                                    <a href="/web/login" class="btn btn-warning btn-lg px-4 py-2">Login</a>
                                </t>
                                <t t-if="not user_id._is_public()">
                                    <a href="/hospital/management" class="btn btn-success btn-lg px-4 py-2 shadow-sm">Go to Live</a>
                                </t>
                            </div>
                        </div>
                        <div class="col-lg-6 text-center">
                            <img src="data:image/jpeg;base64," alt="Medical Team" class="img-fluid rounded shadow-lg"/>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Services Section -->
            <section class="py-5 bg-light">
                <div class="container">
                    <div class="text-center mb-5">
                        <h2 class="display-5 fw-bold">Our Services</h2>
                        <p class="lead text-muted">Comprehensive healthcare solutions tailored to your needs</p>
                    </div>
                    <div class="row g-4">
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0">
                                <img src="https://via.placeholder.com/400x250/28a745/ffffff?text=Patient+Care" class="card-img-top" alt="Patient Care"/>
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Patient Care</h5>
                                    <p class="card-text">Advanced patient management with comprehensive medical records and personalized care plans.</p>
                                    <a href="/hospital/services" class="btn btn-primary">Learn More</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0">
                                <img src="https://via.placeholder.com/400x250/17a2b8/ffffff?text=Laboratory" class="card-img-top" alt="Laboratory"/>
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Laboratory Services</h5>
                                    <p class="card-text">State-of-the-art diagnostic testing with quick and accurate results.</p>
                                    <a href="/hospital/services" class="btn btn-primary">Learn More</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0">
                                <img src="https://via.placeholder.com/400x250/dc3545/ffffff?text=Emergency" class="card-img-top" alt="Emergency"/>
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Emergency Care</h5>
                                    <p class="card-text">24/7 emergency services with experienced medical professionals ready to help.</p>
                                    <a href="/hospital/services" class="btn btn-primary">Learn More</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="text-center mt-4">
                        <a href="/hospital/services" class="btn btn-outline-primary btn-lg">View All Services</a>
                    </div>
                </div>
            </section>

            <!-- Specialists Section -->
            <section class="py-5">
                <div class="container">
                    <div class="text-center mb-5">
                        <h2 class="display-5 fw-bold">Our Specialists</h2>
                        <p class="lead text-muted">Meet our experienced medical professionals</p>
                    </div>
                    <div class="row g-4">
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0 text-center">
                                <img src="https://via.placeholder.com/300x300/6f42c1/ffffff?text=Dr.+Smith" class="card-img-top rounded-circle mx-auto mt-3" alt="Dr. Smith" style="width: 150px; height: 150px;"/>
                                <div class="card-body">
                                    <h5 class="card-title fw-bold">Dr. Taye </h5>
                                    <p class="text-muted">Cardiologist</p>
                                    <p class="card-text">Specializing in heart diseases with over 15 years of experience.</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0 text-center">
                                <img src="https://via.placeholder.com/300x300/20c997/ffffff?text=Dr.+Johnson" class="card-img-top rounded-circle mx-auto mt-3" alt="Dr. Johnson" style="width: 150px; height: 150px;"/>
                                <div class="card-body">
                                    <h5 class="card-title fw-bold">Dr. Michael Johnson</h5>
                                    <p class="text-muted">Neurologist</p>
                                    <p class="card-text">Expert in neurological disorders and brain health.</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0 text-center">
                                <img src="https://via.placeholder.com/300x300/fc6d26/ffffff?text=Dr.+Williams" class="card-img-top rounded-circle mx-auto mt-3" alt="Dr. Williams" style="width: 150px; height: 150px;"/>
                                <div class="card-body">
                                    <h5 class="card-title fw-bold">Dr. Emily Williams</h5>
                                    <p class="text-muted">Pediatrician</p>
                                    <p class="card-text">Dedicated to children's health and development.</p>
                                </div>
                            </div>
                        </div>
                    </div>
                    <div class="text-center mt-4">
                        <a href="/hospital/about" class="btn btn-outline-primary btn-lg">Meet All Specialists</a>
                    </div>
                </div>
            </section>

            <!-- Pricing Section -->
            <section class="py-5 bg-light">
                <div class="container">
                    <div class="text-center mb-5">
                        <h2 class="display-5 fw-bold">Our Packages</h2>
                        <p class="lead text-muted">Affordable healthcare packages for your needs</p>
                    </div>
                    <div class="row g-4">
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0">
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Basic Checkup</h5>
                                    <h3 class="text-primary fw-bold">$99</h3>
                                    <p class="text-muted">per visit</p>
                                    <ul class="list-unstyled">
                                        <li>‚úì General Consultation</li>
                                        <li>‚úì Blood Pressure Check</li>
                                        <li>‚úì Basic Lab Tests</li>
                                        <li>‚úì Health Report</li>
                                    </ul>
                                    <a href="/hospital/services" class="btn btn-primary">Book Now</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0 border-primary">
                                <div class="card-header bg-primary text-white text-center">
                                    <h5 class="mb-0">Most Popular</h5>
                                </div>
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Comprehensive</h5>
                                    <h3 class="text-primary fw-bold">$199</h3>
                                    <p class="text-muted">per visit</p>
                                    <ul class="list-unstyled">
                                        <li>‚úì All Basic Services</li>
                                        <li>‚úì Advanced Lab Tests</li>
                                        <li>‚úì Specialist Consultation</li>
                                        <li>‚úì Follow-up Care</li>
                                    </ul>
                                    <a href="/hospital/services" class="btn btn-primary">Book Now</a>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-4">
                            <div class="card h-100 shadow-sm border-0">
                                <div class="card-body text-center">
                                    <h5 class="card-title fw-bold">Premium Care</h5>
                                    <h3 class="text-primary fw-bold">$299</h3>
                                    <p class="text-muted">per visit</p>
                                    <ul class="list-unstyled">
                                        <li>‚úì All Comprehensive Services</li>
                                        <li>‚úì Home Visits</li>
                                        <li>‚úì 24/7 Support</li>
                                        <li>‚úì Personalized Care Plan</li>
                                    </ul>
                                    <a href="/hospital/services" class="btn btn-primary">Book Now</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </section>

            <!-- Contact Section -->
            <section class="py-5">
                <div class="container">
                    <div class="text-center mb-5">
                        <h2 class="display-5 fw-bold">Get In Touch</h2>
                        <p class="lead text-muted">We're here to help you with your healthcare needs</p>
                    </div>
                    <div class="row g-4">
                        <div class="col-md-4 text-center">
                            <div class="mb-3">
                                <i class="fas fa-phone fa-3x text-primary"></i>
                            </div>
                            <h5>Call Us</h5>
                            <p>+1 (555) 123-4567</p>
                        </div>
                        <div class="col-md-4 text-center">
                            <div class="mb-3">
                                <i class="fas fa-envelope fa-3x text-primary"></i>
                            </div>
                            <h5>Email Us</h5>
                            <p>info@tayehospital.com</p>
                        </div>
                        <div class="col-md-4 text-center">
                            <div class="mb-3">
                                <i class="fas fa-map-marker-alt fa-3x text-primary"></i>
                            </div>
                            <h5>Visit Us</h5>
                            <p>123 Health Street<br/>Medical City, MC 12345</p>
                        </div>
                    </div>
                    <div class="text-center mt-4">
                        <a href="/hospital/contacts" class="btn btn-primary btn-lg">Contact Us Today</a>
                    </div>
                </div>
            </section>
        </t>
    </template>

    <!-- Set hospital home as website homepage -->
    <record id="hospital_homepage" model="website.page">
        <field name="name">Hospital Home</field>
        <field name="url">/</field>
        <field name="view_id" ref="hospital_home_template"/>
        <field name="website_id" ref="website.default_website"/>
        <field name="is_homepage">True</field>
        <field name="website_published">True</field>
    </record>

    <!-- Update default website to use hospital homepage -->
    <record id="website.default_website" model="website">
        <field name="homepage_url">/</field>
    </record>

    <template id="hospital_about_template" name="Hospital About">
        <t t-call="website.layout">
            <div class="container">
                <h1>About Us</h1>
                <p>Information about the hospital.</p>
            </div>
        </t>
    </template>

    <template id="hospital_services_template" name="Hospital Services">
        <t t-call="website.layout">
            <div class="container">
                <h1>Our Services</h1>
                <p>List of services offered.</p>
            </div>
        </t>
    </template>

    <template id="hospital_contacts_template" name="Hospital Contacts">
        <t t-call="website.layout">
            <div class="container">
                <h1>Contact Us</h1>
                <p>Contact information.</p>
            </div>
        </t>
    </template>

    <template id="hospital_blogs_template" name="Hospital Blogs">
        <t t-call="website.layout">
            <div class="container">
                <h1>Blogs</h1>
                <p>Latest blog posts.</p>
            </div>
        </t>
    </template>

    <template id="hospital_news_template" name="Hospital News">
        <t t-call="website.layout">
            <div class="container">
                <h1>News</h1>
                <p>Latest news.</p>
            </div>
        </t>
    </template>

    <template id="doctor_dashboard_template" name="Doctor Dashboard">
        <t t-call="website.layout">
            <div class="container">
                <h1>Doctor Dashboard</h1>
                <p>Welcome, Doctor. Manage your patients and appointments.</p>
                <a href="/web" class="btn btn-primary">Go to Backend</a>
            </div>
        </t>
    </template>

    <template id="patient_dashboard_template" name="Patient Dashboard">
        <t t-call="website.layout">
            <div class="container">
                <h1>Patient Dashboard</h1>
                <p>Welcome, Patient. View your medical records.</p>
                <a href="/web" class="btn btn-primary">Go to Backend</a>
            </div>
        </t>
    </template>

    <template id="admin_dashboard_template" name="Admin Dashboard">
        <t t-call="website.layout">
            <div class="container-fluid py-4" style="background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); min-height: 100vh;">
                <div class="container">
                    <!-- Welcome Header -->
                    <div class="row mb-4">
                        <div class="col-12">
                            <div class="card border-0 shadow-lg" style="background: rgba(255,255,255,0.95); backdrop-filter: blur(10px);">
                                <div class="card-body text-center py-5">
                                    <h1 class="display-4 fw-bold text-primary mb-3">üè• Welcome to Taye Hospital Management</h1>
                                    <p class="lead text-muted mb-4">Comprehensive Hospital Information System - Manage Patients, Staff, and Operations</p>
                                    <a href="/web" class="btn btn-primary btn-lg px-5 py-3 shadow-sm fw-bold">
                                        <i class="fas fa-cogs me-2"></i>Access Full Hospital System
                                    </a>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Statistics Cards -->
                    <div class="row mb-4">
                        <div class="col-md-3 mb-3">
                            <div class="card border-0 shadow-sm h-100" style="background: linear-gradient(135deg, #ff9a9e 0%, #fecfef 100%);">
                                <div class="card-body text-center text-dark">
                                    <i class="fas fa-users fa-3x mb-3"></i>
                                    <h3 class="fw-bold">1,250+</h3>
                                    <p class="mb-0">Registered Patients</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 mb-3">
                            <div class="card border-0 shadow-sm h-100" style="background: linear-gradient(135deg, #a8edea 0%, #fed6e3 100%);">
                                <div class="card-body text-center text-dark">
                                    <i class="fas fa-user-md fa-3x mb-3"></i>
                                    <h3 class="fw-bold">45</h3>
                                    <p class="mb-0">Medical Staff</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 mb-3">
                            <div class="card border-0 shadow-sm h-100" style="background: linear-gradient(135deg, #ffecd2 0%, #fcb69f 100%);">
                                <div class="card-body text-center text-dark">
                                    <i class="fas fa-calendar-check fa-3x mb-3"></i>
                                    <h3 class="fw-bold">320</h3>
                                    <p class="mb-0">Today's Appointments</p>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-3 mb-3">
                            <div class="card border-0 shadow-sm h-100" style="background: linear-gradient(135deg, #d299c2 0%, #fef9d7 100%);">
                                <div class="card-body text-center text-dark">
                                    <i class="fas fa-bed fa-3x mb-3"></i>
                                    <h3 class="fw-bold">85</h3>
                                    <p class="mb-0">Available Beds</p>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Quick Actions -->
                    <div class="row mb-4">
                        <div class="col-12">
                            <div class="card border-0 shadow-lg">
                                <div class="card-header bg-primary text-white">
                                    <h5 class="mb-0"><i class="fas fa-bolt me-2"></i>Quick Actions</h5>
                                </div>
                                <div class="card-body">
                                    <div class="row g-3">
                                        <div class="col-md-3">
                                            <a href="/web#menu_id=hospital_management.menu_hospital_patients" class="btn btn-outline-primary btn-lg w-100 p-3">
                                                <i class="fas fa-user-plus fa-2x mb-2"></i><br/>
                                                <strong>Add New Patient</strong>
                                            </a>
                                        </div>
                                        <div class="col-md-3">
                                            <a href="/web#menu_id=hospital_management.menu_hospital_appointments" class="btn btn-outline-success btn-lg w-100 p-3">
                                                <i class="fas fa-calendar-plus fa-2x mb-2"></i><br/>
                                                <strong>Schedule Appointment</strong>
                                            </a>
                                        </div>
                                        <div class="col-md-3">
                                            <a href="/web#menu_id=hospital_management.menu_hospital_lab_tests" class="btn btn-outline-info btn-lg w-100 p-3">
                                                <i class="fas fa-flask fa-2x mb-2"></i><br/>
                                                <strong>Order Lab Test</strong>
                                            </a>
                                        </div>
                                        <div class="col-md-3">
                                            <a href="/web#menu_id=hospital_management.menu_hospital_medical_records" class="btn btn-outline-warning btn-lg w-100 p-3">
                                                <i class="fas fa-file-medical fa-2x mb-2"></i><br/>
                                                <strong>View Records</strong>
                                            </a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- System Features -->
                    <div class="row">
                        <div class="col-12">
                            <div class="card border-0 shadow-lg">
                                <div class="card-header bg-success text-white">
                                    <h5 class="mb-0"><i class="fas fa-star me-2"></i>System Features</h5>
                                </div>
                                <div class="card-body">
                                    <div class="row">
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Patient Management</h6>
                                                    <small class="text-muted">Complete patient registration and tracking</small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Electronic Health Records</h6>
                                                    <small class="text-muted">Digital medical records and history</small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Appointment Scheduling</h6>
                                                    <small class="text-muted">Automated appointment management</small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Laboratory Integration</h6>
                                                    <small class="text-muted">Seamless lab test ordering and results</small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Pharmacy Management</h6>
                                                    <small class="text-muted">Medication dispensing and inventory</small>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4 mb-3">
                                            <div class="d-flex align-items-center">
                                                <i class="fas fa-check-circle text-success fa-2x me-3"></i>
                                                <div>
                                                    <h6 class="mb-1">Billing &amp; Invoicing</h6>
                                                    <small class="text-muted">Automated billing and payment processing</small>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </t>
    </template>

    <!-- Other hospital pages -->
